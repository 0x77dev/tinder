{
  "author": "Mykhailo Marynenko @0x77dev",
  "bin": {
    "tinder": "./bin/run"
  },
  "bugs": "https://github.com/0x77dev/tinder/issues",
  "dependencies": {
    "@oclif/core": "^1.16.5",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.1.1",
    "@tinder-tools/api": "workspace:api",
    "anyproxy": "^4.1.3",
    "conf": "^10.2.0",
    "cross-fetch": "^3.1.5",
    "exit-hook": "^3.1.0",
    "signal-exit": "^3.0.7"
  },
  "description": "Tinder CLI",
  "devDependencies": {
    "@oclif/test": "^2.2.2",
    "@types/anyproxy": "^4",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.64",
    "@types/signal-exit": "^3.0.1",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.3.1",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/0x77dev/tinder",
  "keywords": [
    "oclif",
    "tinder"
  ],
  "license": "GPL-3.0",
  "main": "dist/index.js",
  "name": "@tinder-tools/console",
  "oclif": {
    "bin": "tinder",
    "commands": "./dist/commands",
    "dirname": "cli",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "api": {
        "description": "Perform an API requests, for example: get user info"
      }
    }
  },
  "repository": "0x77dev/tinder",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts",
  "version": "0.0.0"
}
